<div class="main users-index">
  <div class="container">

<h1><%= @youtube_data.items[0].snippet.channel_title %></h1>
<h1><%= @youtube_data.items[0].snippet.thumbnails.default.url %></h1>
 <img src="<%= @youtube_data.items[0].snippet.thumbnails.default.url %>">
<h1><%= @youtube_data.items[0].snippet.description %></h1>

    <p><%= @youtube_data.items %></p>

    <% @youtube_data.items.each do |item| %>
      <% snippet = item.snippet %>
      <h4><%= snippet.title %></h4>
      <p><%= snippet.published_at %><%= snippet.channel_title %></p>
      <div><iframe width="560" height="315" src="https://www.youtube.com/embed/<%= item.id.video_id %>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe></div>
    <% end %>
  </div>
</div>
